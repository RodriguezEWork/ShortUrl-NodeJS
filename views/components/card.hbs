<article class="card mb-2">
    <div class="card-body">
        <p>{{ origin }}</p>
        <p>{{ shortUrl }}</p>
        <button class="btn btn-info" data-short="{{shortUrl}}">Copiar</button>
        <a href="/editar/{{_id}}" class="btn btn-warning">Editar</a>
        <a href="/eliminar/{{_id}}" class="btn btn-danger">Eliminar</a>
    </div>
</article>